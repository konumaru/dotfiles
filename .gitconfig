[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true

[alias]
    ch = checkout
    br = branch
    a = add -uA
    c = commit -m

[init]
    defaultBranc = main
    defaultBranch = main

[ghq]
    root = ~/Documents/repositories
